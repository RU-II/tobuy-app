  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start Next.js",
      "type": "pwa-node",
      "request": "launch",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["dev"],
      "cwd": "${workspaceFolder}//front"
    },
    {
      "name": "Start Golang Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}//api/server"
    },
    {
      "name": "Start Mock Server",
      "type": "pwa-node",
      "request": "launch",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["start"],
      "cwd": "${workspaceFolder}//api/mock"
    }
  ]
}
